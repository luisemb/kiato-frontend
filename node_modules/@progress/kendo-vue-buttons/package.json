{
  "name": "@progress/kendo-vue-buttons",
  "description": "Kendo UI for Vue Buttons package",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-vue.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "exports": {
    "node": "./dist/esm/main.js",
    "import": "./dist/es/main.js",
    "require": "./dist/npm/main.js",
    "default": "./dist/npm/main.js"
  },
  "scripts": {
    "build-package": "gulp build-package",
    "start": "gulp start --notsc",
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/buttons/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.config.e2e.js packages/buttons"
  },
  "homepage": "https://www.telerik.com/kendo-vue-ui/",
  "keywords": [
    "Kendo UI",
    "Vue",
    "Progress",
    "Buttons",
    "KendoVue",
    "vuejs",
    "UI",
    "components",
    "Vue component",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "vue": "^2.6.12 || ^3.0.2"
  },
  "dependencies": {
    "@progress/kendo-vue-common": "4.0.0",
    "@progress/kendo-vue-popup": "4.0.0"
  },
  "devDependencies": {
    "@progress/kendo-drawing": "^1.5.12",
    "@progress/kendo-svg-icons": "^2.0.0",
    "@progress/kendo-vue-dropdowns": "4.0.0",
    "@progress/kendo-vue-inputs": "4.0.0",
    "@progress/kendo-vue-layout": "4.0.0"
  },
  "@progress": {
    "friendlyName": "Buttons",
    "framework": "Kendo UI for Vue"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  }
}
