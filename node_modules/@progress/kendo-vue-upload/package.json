{
  "name": "@progress/kendo-vue-upload",
  "description": "Kendo UI for Vue Upload package",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-vue.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "exports": {
    "node": "./dist/esm/main.js",
    "import": "./dist/es/main.js",
    "require": "./dist/npm/main.js",
    "default": "./dist/npm/main.js"
  },
  "scripts": {
    "build-package": "gulp build-package",
    "start": "gulp start --notsc",
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/upload/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.config.e2e.js packages/upload"
  },
  "keywords": [
    "Kendo UI",
    "Vue",
    "Progress",
    "Upload"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "vue": "^2.6.12 || ^3.0.2"
  },
  "dependencies": {
    "@progress/kendo-vue-buttons": "4.0.0",
    "@progress/kendo-vue-common": "4.0.0",
    "@progress/kendo-vue-progressbars": "4.0.0",
    "axios": "0.24.0"
  },
  "devDependencies": {
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "@progress/kendo-vue-intl": "4.0.0"
  },
  "@progress": {
    "friendlyName": "Upload",
    "framework": "Kendo UI for Vue"
  },
  "author": "Progress",
  "license": "Commercial",
  "publishConfig": {
    "access": "public"
  }
}
