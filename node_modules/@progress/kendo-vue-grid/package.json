{
  "name": "@progress/kendo-vue-grid",
  "version": "4.0.0",
  "description": "Kendo UI for Vue Grid package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-vue.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "exports": {
    "node": "./dist/esm/main.js",
    "import": "./dist/es/main.js",
    "require": "./dist/npm/main.js",
    "default": "./dist/npm/main.js"
  },
  "scripts": {
    "build-package": "gulp build-package",
    "start": "gulp start --notsc",
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/grid/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.config.e2e.js packages/grid"
  },
  "keywords": [
    "Kendo UI",
    "Vue",
    "Grid",
    "Progress"
  ],
  "peerDependencies": {
    "@progress/kendo-data-query": "^1.5.4",
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "@progress/kendo-vue-animation": "^4.0.0",
    "@progress/kendo-vue-buttons": "^4.0.0",
    "@progress/kendo-vue-data-tools": "^4.0.0",
    "@progress/kendo-vue-dateinputs": "^4.0.0",
    "@progress/kendo-vue-dropdowns": "^4.0.0",
    "@progress/kendo-vue-indicators": "^4.0.0",
    "@progress/kendo-vue-inputs": "^4.0.0",
    "@progress/kendo-vue-intl": "^4.0.0",
    "@progress/kendo-vue-popup": "^4.0.0",
    "vue": "^2.6.12 || ^3.0.2"
  },
  "dependencies": {
    "@progress/kendo-vue-common": "4.0.0"
  },
  "devDependencies": {
    "@progress/kendo-data-query": "^1.5.4",
    "@progress/kendo-drawing": "^1.9.3",
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "@progress/kendo-vue-animation": "4.0.0",
    "@progress/kendo-vue-buttons": "4.0.0",
    "@progress/kendo-vue-charts": "4.0.0",
    "@progress/kendo-vue-data-tools": "4.0.0",
    "@progress/kendo-vue-dateinputs": "4.0.0",
    "@progress/kendo-vue-dropdowns": "4.0.0",
    "@progress/kendo-vue-excel-export": "4.0.0",
    "@progress/kendo-vue-indicators": "4.0.0",
    "@progress/kendo-vue-inputs": "4.0.0",
    "@progress/kendo-vue-intl": "4.0.0",
    "@progress/kendo-vue-pdf": "4.0.0",
    "@progress/kendo-vue-popup": "4.0.0",
    "cldr-core": "^41.0.0",
    "cldr-dates-full": "^41.0.0",
    "cldr-numbers-full": "^41.0.0",
    "hammerjs": "^2.0.0"
  },
  "@progress": {
    "friendlyName": "Grid",
    "framework": "Kendo UI for Vue"
  },
  "author": "Progress",
