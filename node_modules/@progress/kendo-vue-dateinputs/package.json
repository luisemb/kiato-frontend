{
  "name": "@progress/kendo-vue-dateinputs",
  "description": "Kendo UI for Vue Date Inputs package",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-vue.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "exports": {
    "node": "./dist/esm/main.js",
    "import": "./dist/es/main.js",
    "require": "./dist/npm/main.js",
    "default": "./dist/npm/main.js"
  },
  "scripts": {
    "build-package": "gulp build-package",
    "start": "gulp start --notsc",
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/dateinputs/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.config.e2e.js packages/dateinputs"
  },
  "keywords": [
    "Kendo UI",
    "Vue",
    "Progress",
    "KendoVue",
    "vuejs",
    "UI",
    "components",
    "Vue component",
    "Telerik",
    "Date Inputs",
    "Vue component",
    "Telerik",
    "DatePicker",
    "form valildation",
    "date",
    "picker",
    "datetime",
    "calendar"
  ],
  "@progress": {
    "friendlyName": "Date Inputs",
    "framework": "Kendo UI for Vue"
  },
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "@progress/kendo-vue-intl": "^4.0.0",
    "vue": "^2.6.12 || ^3.0.2"
  },
  "dependencies": {
    "@progress/kendo-date-math": "1.5.1",
    "@progress/kendo-vue-buttons": "4.0.0",
    "@progress/kendo-vue-common": "4.0.0",
    "@progress/kendo-vue-labels": "4.0.0",
    "@progress/kendo-vue-popup": "4.0.0"
  },
  "devDependencies": {
    "@progress/kendo-data-query": "^1.5.4",
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-svg-icons": "^2.0.0",
    "@progress/kendo-vue-dropdowns": "4.0.0",
    "@progress/kendo-vue-intl": "4.0.0"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  }
}
