L,uBAAe,CAACG,OAAhB,CAAwB,UAAUG,GAAV,EAAe;AACnCA,aAAG,CAACC,SAAJ,CAAcI,MAAd,CAAqB,gBAArB;AACH,SAFD;AAGH,OAJD;AAKH,KAND;AAOD;AA7HY,CAAf","names":["flatPickr","SearchIcon","InfoIcon","MoreVerticalIcon","required","helpers","useVuelidate","Layout","appConfig","chatData","chatMessagesData","setup","v$","page","title","meta","name","content","description","data","items","text","href","active","submitted","form","message","username","profile","require","dateTimeConfig","enableTime","dateFormat","humanfriendlyConfig2","date","config","wrap","altFormat","altInput","components","validations","withMessage","methods","chatUsername","image","usermessage","currentDate","Date","push","time","getHours","getMinutes","formSubmit","$touch","$invalid","align","mounted","userChatElement","document","querySelectorAll","forEach","item","addEventListener","elm","classList","add","chatUserList","querySelector","remove","parentNode"],"sourceRoot":"","sources":["C:\\wamp64\\www\\crm\\frontend\\src\\views\\apps\\chat.vue"],"sourcesContent":["<script>\n\nimport flatPickr from \"vue-flatpickr-component\";\n\nimport \"flatpickr/dist/flatpickr.css\";\n\nimport {\n  SearchIcon,\n  InfoIcon,\n  MoreVerticalIcon,\n} from \"@zhuowenli/vue-feather-icons\";\n\nimport { required, helpers } from \"@vuelidate/validators\";\nimport useVuelidate from \"@vuelidate/core\";\n\nimport Layout from \"../../layouts/main.vue\";\nimport appConfig from \"../../../app.config\";\n\nimport { chatData, chatMessagesData } from \"./data\";\n\nexport default {\n  setup() {\n    return { v$: useVuelidate() };\n  },\n  page: {\n    title: \"Chat\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  data() {\n    return {\n      chatData: chatData,\n      chatMessagesData: chatMessagesData,\n      title: \"Chat\",\n      items: [\n        {\n          text: \"Velzon\",\n          href: \"/\",\n        },\n        {\n          text: \"Chat\",\n          active: true,\n        },\n      ],\n      submitted: false,\n      form: {\n        message: \"\",\n      },\n      username: \"Steven Franklin\",\n      profile: require(\"@/assets/images/users/avatar-2.jpg\"),\n      dateTimeConfig: {\n        enableTime: true,\n        dateFormat: \"d.m.y H:i\",\n      },\n      humanfriendlyConfig2: {\n        dateFormat: \"F j, Y\",\n      },\n      date: null,\n      config: {\n        wrap: true, // set wrap to true only when using 'input-group'\n        altFormat: \"M j, Y\",\n        altInput: true,\n        dateFormat: \"d M, Y\",\n      },\n    };\n  },\n  components: {\n    Layout,\n    SearchIcon,\n    InfoIcon,\n    MoreVerticalIcon,\n    flatPickr,\n  },\n  validations: {\n    form: {\n      message: {\n        required: helpers.withMessage(\"Message is required\", required),\n      },\n    },\n  },\n  methods: {\n    /**\n     * Get the name of user\n     */\n    chatUsername(name, image) {\n      this.username = name;\n      this.profile = image;\n      this.usermessage = \"Hello\";\n      this.chatMessagesData = [];\n      const currentDate = new Date();\n\n      this.chatMessagesData.push({\n        name: this.username,\n        message: this.usermessage,\n        time: currentDate.getHours() + \":\" + currentDate.getMinutes(),\n      });\n    },\n\n    /**\n     * Char form Submit\n     */\n    formSubmit() {\n      this.submitted = true;\n\n      // stop here if form is invalid\n      this.v$.$touch();\n\n      if (this.v$.$invalid) {\n        return;\n      } else {\n        const message = this.form.message;\n        const currentDate = new Date();\n        this.chatMessagesData.push({\n          align: \"right\",\n          name: \"Henry Wells\",\n          message,\n          time: currentDate.getHours() + \":\" + currentDate.getMinutes(),\n        });\n      }\n      this.submitted = false;\n      this.form = {};\n    },\n  },\n  mounted() {\n    var userChatElement = document.querySelectorAll(\".user-chat\");\n    document.querySelectorAll(\".chat-user-list li a\").forEach(function (item) {\n        item.addEventListener(\"click\", function () {\n            userChatElement.forEach(function (elm) {\n                elm.classList.add(\"user-chat-show\");\n            });\n\n            // chat user list link active\n            var chatUserList = document.querySelector(\".chat-user-list li.active\");\n            if (chatUserList) chatUserList.classList.remove(\"active\");\n            this.parentNode.classList.add(\"active\");\n        });\n    });\n\n    // user-chat-remove\n    document.querySelectorAll(\".user-chat-remove\").forEach(function (item) {\n        item.addEventListener(\"click\", function () {\n            userChatElement.forEach(function (elm) {\n                elm.classList.remove(\"user-chat-show\");\n            });\n        });\n    });\n  },\n};\n</script>\n\n<template>\n  <Layout>\n\n    <div class=\"col-lg-6\">\n      <div>\n        <label class=\"form-label mb-0\">Basic</label>\n        <p class=\"text-muted\">\n          Set\n          <code\n          >data-provider=\"flatpickr\" data-date-format=\"d M,\n            Y\"</code\n          >\n          attribute.\n        </p>\n        <flat-pickr\n            v-model=\"date\"\n            :config=\"config\"\n            class=\"form-control flatpickr-input\"\n        ></flat-pickr>\n      </div>\n    </div>\n\n    <div class=\"chat-wrapper d-lg-flex gap-1 mx-n4 mt-n4 p-1\">\n      <div class=\"chat-leftsidebar\">\n        <div class=\"px-4 pt-4 mb-4\">\n          <div class=\"d-flex align-items-start\">\n            <div class=\"flex-grow-1\">\n              <h5 class=\"mb-4\">Chats</h5>\n            </div>\n            <div class=\"flex-shrink-0\">\n              <div\n                data-bs-toggle=\"tooltip\"\n                data-bs-trigger=\"hover\"\n                data-bs-placement=\"bottom\"\n                title=\"Add Contact\"\n              >\n                <!-- Button trigger modal -->\n                <button type=\"button\" class=\"btn btn-soft-primary btn-sm\">\n                  <i class=\"ri-add-line align-bottom\"></i>\n                </button>\n              </div>\n            </div>\n          </div>\n          <div class=\"search-box\">\n            <input\n              type=\"text\"\n              class=\"form-control bg-light border-light\"\n              placeholder=\"Search here...\"\n            />\n            <i class=\"ri-search-2-line search-icon\"></i>\n          </div>\n        </div>\n        <!-- .p-4 -->\n\n        <div class=\"chat-room-list\" data-simplebar>\n          <div class=\"d-flex align-items-center px-4 mb-2\">\n            <div class=\"flex-grow-1\">\n              <h4 class=\"mb-0 fs-11 text-muted text-uppercase\">\n                Direct Messages\n              </h4>\n            </div>\n            <div class=\"flex-shrink-0\">\n              <div\n                data-bs-toggle=\"tooltip\"\n                data-bs-trigger=\"hover\"\n                data-bs-placement=\"bottom\"\n                title=\"New Message\"\n              >\n                <!-- Button trigger modal -->\n                <button type=\"button\" class=\"btn btn-soft-primary btn-sm\">\n                  <i class=\"ri-add-line align-bottom\"></i>\n                </button>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"chat-message-list\">\n            <SimpleBar class=\"list-unstyled chat-list chat-user-list\">\n              <li\n                class\n                v-for=\"data of chatData\"\n                :key=\"data.id\"\n                @click=\"chatUsername(data.name, data.image)\"\n                :class=\"{ active: username == data.name }\"\n              >\n                <a href=\"javascript: void(0);\">\n                  <div class=\"d-flex align-items-center\">\n                    <div\n                      class=\"\n                        flex-shrink-0\n                        chat-user-img\n                        online\n                        align-self-center\n                        me-2\n                        ms-0\n                      \"\n                    >\n                      <div class=\"avatar-xxs\" v-if=\"data.image\">\n                        <img\n                          :src=\"`${data.image}`\"\n                          class=\"rounded-circle img-fluid userprofile\"\n                          alt\n                        />\n                      </div>\n                      <div class=\"avatar-xxs\" v-if=\"!data.image\">\n                        <div\n                          class=\"\n                            avatar-title\n                            rounded-circle\n                            bg-danger\n                            userprofile\n                          \"\n                        >\n                          {{ data.name.charAt(0) }}\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"flex-grow-1 overflow-hidden\">\n                      <p class=\"text-truncate mb-1\">\n                        {{ data.name }}\n                      </p>\n                    </div>\n\n                    <div class=\"flex-shrink-0\">\n                          <span class=\"badge badge-soft-dark rounded p-1\"\n                            >{{data.time}}</span\n                          >\n                        </div>\n                  </div>\n                </a>\n              </li>\n            </SimpleBar>\n          </div>\n\n          <div class=\"d-flex align-items-center px-4 mt-4 pt-2 mb-2\">\n            <div class=\"flex-grow-1\">\n              <h4 class=\"mb-0 fs-11 text-muted text-uppercase\">Channels</h4>\n            </div>\n            <div class=\"flex-shrink-0\">\n              <div\n                data-bs-toggle=\"tooltip\"\n                data-bs-trigger=\"hover\"\n                data-bs-placement=\"bottom\"\n                title=\"Create group\"\n              >\n                <!-- Button trigger modal -->\n                <button type=\"button\" class=\"btn btn-soft-primary btn-sm\">\n                  <i class=\"ri-add-line align-bottom\"></i>\n                </button>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"chat-message-list\">\n            <ul\n              class=\"list-unstyled chat-list chat-user-list mb-0\"\n              id=\"channelList\"\n            >\n              <li>\n                <a href=\"javascript: void(0);\" class=\"unread-msg-user\">\n                  <div class=\"d-flex align-items-center\">\n                    <div\n                      class=\"\n                        flex-shrink-0\n                        chat-user-img\n                        online\n                        align-self-center\n                        me-2\n                        ms-0\n                      \"\n                    >\n                      <div class=\"avatar-xxs\">\n                        <div\n                          class=\"avatar-title bg-light rounded-circle text-body\"\n                        >\n                          #\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"flex-grow-1 overflow-hidden\">\n                      <p class=\"text-truncate mb-0\">Landing Design</p>\n                    </div>\n                    <div class=\"flex-shrink-0\">\n                      <span class=\"badge badge-soft-dark rounded p-1\">7</span>\n                    </div>\n                  </div>\n                </a>\n              </li>\n              <li>\n                <a href=\"javascript: void(0);\">\n                  <div class=\"d-flex align-items-center\">\n                    <div\n                      class=\"\n                        flex-shrink-0\n                        chat-user-img\n                        online\n                        align-self-center\n                        me-2\n                        ms-0\n                      \"\n                    >\n                      <div class=\"avatar-xxs\">\n                        <div\n                          class=\"avatar-title bg-light rounded-circle text-body\"\n                        >\n                          #\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"flex-grow-1 overflow-hidden\">\n                      <p class=\"text-truncate mb-0\">General</p>\n                    </div>\n                  </div>\n                </a>\n              </li>\n              <li>\n                <a href=\"javascript: void(0);\" class=\"unread-msg-user\">\n                  <div class=\"d-flex align-items-center\">\n                    <div\n                      class=\"\n                        flex-shrink-0\n                        chat-user-img\n                        online\n                        align-self-center\n                        me-2\n                        ms-0\n                      \"\n                    >\n                      <div class=\"avatar-xxs\">\n                        <div\n                          class=\"avatar-title bg-light rounded-circle text-body\"\n                        >\n                          #\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"flex-grow-1 overflow-hidden\">\n                      <p class=\"text-truncate mb-0\">Project Tasks</p>\n                    </div>\n                    <div class=\"flex-shrink-0\">\n                      <span class=\"badge badge-soft-dark rounded p-1\">3</span>\n                    </div>\n                  </div>\n                </a>\n              </li>\n\n              <li>\n                <a href=\"javascript: void(0);\">\n                  <div class=\"d-flex align-items-center\">\n                    <div\n                      class=\"\n                        flex-shrink-0\n                        chat-user-img\n                        online\n                        align-self-center\n                        me-2\n                        ms-0\n                      \"\n                    >\n                      <div class=\"avatar-xxs\">\n                        <div\n                          class=\"avatar-title bg-light rounded-circle text-dark\"\n                        >\n                          #\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"flex-grow-1 overflow-hidden\">\n                      <p class=\"text-truncate mb-0\">Meeting</p>\n                    </div>\n                  </div>\n                </a>\n              </li>\n              <li>\n                <a href=\"javascript: void(0);\">\n                  <div class=\"d-flex align-items-center\">\n                    <div\n                      class=\"\n                        flex-shrink-0\n                        chat-user-img\n                        online\n                        align-self-center\n                        me-2\n                        ms-0\n                      \"\n                    >\n                      <div class=\"avatar-xxs\">\n                        <div\n                          class=\"avatar-title bg-light rounded-circle text-dark\"\n                        >\n                          #\n                        </div>\n                      </div>\n                    </div>\n                    <div class=\"flex-grow-1 overflow-hidden\">\n                      <p class=\"text-truncate mb-0\">Reporting</p>\n                    </div>\n                  </div>\n                </a>\n              </li>\n            </ul>\n          </div>\n          <!-- End chat-message-list -->\n        </div>\n      </div>\n      <!-- end chat leftsidebar -->\n      <!-- Start User chat -->\n      <div class=\"user-chat w-100 overflow-hidden\">\n        <div class=\"chat-content d-lg-flex\">\n          <!-- start chat conversation section -->\n          <div class=\"w-100 overflow-hidden position-relative\">\n            <!-- conversation user -->\n            <div class=\"position-relative\">\n              <div class=\"p-3 user-chat-topbar\">\n                <div class=\"row align-items-center\">\n                  <div class=\"col-sm-4 col-8\">\n                    <div class=\"d-flex align-items-center\">\n                      <div class=\"flex-shrink-0 d-block d-lg-none me-3\">\n                        <a\n                          href=\"javascript: void(0);\"\n                          class=\"user-chat-remove fs-18 p-1\"\n                          ><i class=\"ri-arrow-left-s-line align-bottom\"></i\n                        ></a>\n                      </div>\n                      <div class=\"flex-grow-1 overflow-hidden\">\n                        <div class=\"d-flex align-items-center\">\n                          <div\n                            class=\"\n                              flex-shrink-0\n                              chat-user-img\n                              online\n                              user-own-img\n                              align-self-center\n                              me-3\n                              ms-0\n                            \"\n                          >\n                            <img\n                              :src=\"\n                                profile\n                                  ? profile\n                                  : require('@/assets/images/users/user-dummy-img.jpg')\n                              \"\n                              class=\"rounded-circle avatar-xs\"\n                              alt=\"\"\n                            />\n                            <span class=\"user-status\"></span>\n                          </div>\n                          <div class=\"flex-grow-1 overflow-hidden\">\n                            <h5 class=\"text-truncate mb-0 fs-16\">\n                              <a\n                                class=\"text-reset username\"\n                                data-bs-toggle=\"offcanvas\"\n                                href=\"#userProfileCanvasExample\"\n                                aria-controls=\"userProfileCanvasExample\"\n                                >{{ username }}</a\n                              >\n                            </h5>\n                            <p\n                              class=\"\n                                text-truncate text-muted\n                                fs-14\n                                mb-0\n                                userStatus\n                              \"\n                            >\n                              <small>Online</small>\n                            </p>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"col-sm-8 col-4\">\n                    <ul class=\"list-inline user-chat-nav text-end mb-0\">\n                      <li class=\"list-inline-item m-0\">\n                        <div class=\"dropdown\">\n                          <button\n                            class=\"btn btn-ghost-secondary btn-icon\"\n                            type=\"button\"\n                            data-bs-toggle=\"dropdown\"\n                            aria-haspopup=\"true\"\n                            aria-expanded=\"false\"\n                          >\n                            <search-icon class=\"icon-sm\"></search-icon>\n                          </button>\n                          <div\n                            class=\"\n                              dropdown-menu\n                              p-0\n                              dropdown-menu-end dropdown-menu-lg\n                            \"\n                          >\n                            <div class=\"p-2\">\n                              <div class=\"search-box\">\n                                <input\n                                  type=\"text\"\n                                  class=\"form-control bg-light border-light\"\n                                  placeholder=\"Search here...\"\n                                  onkeyup=\"searchMessages()\"\n                                  id=\"searchMessage\"\n                                />\n                                <i class=\"ri-search-2-line search-icon\"></i>\n                              </div>\n                            </div>\n                          </div>\n                        </div>\n                      </li>\n\n                      <li class=\"list-inline-item d-none d-lg-inline-block m-0\">\n                        <button\n                          type=\"button\"\n                          class=\"btn btn-ghost-secondary btn-icon\"\n                          data-bs-toggle=\"offcanvas\"\n                          data-bs-target=\"#userProfileCanvasExample\"\n                          aria-controls=\"userProfileCanvasExample\"\n                        >\n                          <info-icon class=\"icon-sm\"></info-icon>\n                        </button>\n                      </li>\n\n                      <li class=\"list-inline-item m-0\">\n                        <div class=\"dropdown\">\n                          <button\n                            class=\"btn btn-ghost-secondary btn-icon\"\n                            type=\"button\"\n                            data-bs-toggle=\"dropdown\"\n                            aria-haspopup=\"true\"\n                            aria-expanded=\"false\"\n                          >\n                            <more-vertical-icon\n                              class=\"icon-sm\"\n                            ></more-vertical-ico