======================= -->\n\n    <div class=\"main-content\">\n      <div class=\"page-content\">\n        <!-- Start Content-->\n        <div class=\"container-fluid\">\n          <slot />\n        </div>\n      </div>\n      <Footer />\n    </div>\n    <RightBar />\n  </div>\n</template>"]}]}